# Ethereum RPC URL for Sepolia testnet
# Get one from: https://infura.io or https://alchemy.com
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY_HERE

# Your wallet's private key (NEVER commit the real .env file!)
# Export from MetaMask: Account Details > Export Private Key
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Etherscan API key for contract verification
# Get one from: https://etherscan.io/myapikey
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_API_KEY_HERE
